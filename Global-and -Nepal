   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "def day_wise_sum(df,col_name):\n",
    "    #Function which count the specific  given case(ie confirmed or deaths or recovered)......\n",
    "    #for each day  and returns the dataframe with date and respective  total case \n",
    "    \n",
    "    df2=df.copy().drop(['Province/State','Country/Region','Lat','Long'],axis=1).sum()\n",
    "    df2=pd.DataFrame(df2)\n",
    "    df2.rename(columns={0:col_name},inplace=True)\n",
    "    df2.index.names=['Date']\n",
    "    df2.index=pd.to_datetime(df2.index)\n",
    "    return df2    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "confirmed=day_wise_sum(confirmed_df,'confirmed_cases')\n",
    "recovered=day_wise_sum(recovered_df,'recovered_cases')\n",
    "deaths=day_wise_sum(deaths_df,'deaths_cases')"
   ]
  },
