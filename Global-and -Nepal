    "# Confirmed,deaths,recovered cases over a time in nepal"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "metadata": {},
   "outputs": [],
   "source": [
    "def global_to_nepal(df,col_name):\n",
    "    #From each respective case df finds data of nepal\n",
    "    #returned df contains respective case  of each day\n",
    "    df_nepal=df[df['Country/Region']=='Nepal'].copy()\n",
    "    df_nepal.drop(['Province/State','Country/Region','Lat','Long'],axis=1,inplace=True)\n",
    "    df_nepal=df_nepal.T\n",
    "    df_nepal.columns=[col_name]\n",
    "    df_nepal.index.names=['Date']\n",
    "    return df_nepal\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "confirmed_nepal=global_to_nepal(confirmed_df,'confirmed_cases')\n",
    "recovered_nepal=global_to_nepal(recovered_df,'recovered_cases')\n",
    "deaths_nepal=global_to_nepal(deaths_df,'deaths_cases')"
   ]
  },
